{
	"language": "en-GB",
	"ignorePaths": [
		"**/node_modules/**",
		"LICENSE",
		"**/coverage/**",
		"**/dist/**",
		"**/docs/reference/**",
		"**/docs/open-api/**",
		"**/tests/**",
		"*.yaml",
		"package.json",
		"rollup.config.mjs",
		"vitest.config.*",
		"tsconfig.json",
		"typedoc.json",
		".eslintrc.cjs",
		"**/wordlists/**",
		"changelog",
		"devnet.env.*",
		"mainnet.env.*",
		"testnet.env.*"
	],
	"words": [
		"abbrev",
		"adduser",
		"allkeys",
		"autorelease",
		"autorestart",
		"bech",
		"BGSAVE",
		"BUILDKIT",
		"buildx",
		"childlogdir",
		"Cleanup",
		"envsubst",
		"FLUSHALL",
		"FLUSHDB",
		"iotaledger",
		"keepalive",
		"loglevel",
		"maxmemory",
		"multiarch",
		"nameof",
		"nodaemon",
		"pidfile",
		"procps",
		"requirepass",
		"rpcinterface",
		"Secp",
		"serverurl",
		"startretries",
		"Stiftung",
		"supervisorctl",
		"supervisord",
		"twinfoundation",
		"Nanos",
		"devnet",
		"FAUCET"
	],
	"ignoreRegExpList": [
		"/from\\s+(['\"]).*\\1/",
		"/eslint-disable.*/",
		"/^```shell[\\s\\S]*?```$/gm",
		"/^```json[\\s\\S]*?```$/gm",
		"/tst1.*/"
	]
}
